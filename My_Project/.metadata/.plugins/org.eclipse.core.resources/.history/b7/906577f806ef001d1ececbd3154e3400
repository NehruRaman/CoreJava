package pattern;

public class Pattern2 {

	public static void main(String[] args) {
		// TODO Auto-generated method stub

	}
	public void pattern()
	{
		for(int row = 1;row <= 5;row++)
		{
			for(int col = 1;col <= 5;col++)
			{
			System.out.print(row);	
			
			} 
      System.out.println("");
	}

}

}
